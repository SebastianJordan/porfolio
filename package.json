{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters='json-summary' ",
    "test:badges": "npm run test:coverage && jest-coverage-badges output './badges'",
    "publish": "vercel deploy --prod  --token=$VERCEL_TOKEN"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "emailjs": "^4.0.1",
    "firebase": "^9.10.0",
    "firebase-admin": "^11.0.1",
    "jest-coverage-badges": "^1.1.2",
    "next": "12.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "vercel": "^28.2.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "autoprefixer": "^10.4.11",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8"
  }
}
